@model Note
@{
    ViewData["Title"] = "NoteEdit";
}

<h2>Edit Note</h2>

<form method="post">
    <input type="hidden" asp-for="ID" />
    <input type="hidden" asp-for="From" />
    <input type="hidden" asp-for="ProjectID" />
    <input type="hidden" asp-for="Date" />
    <label asp-for="Text">Note:</label>
    <br /><textarea asp-for="Text"></textarea>
    <br /><input type="submit" value="Update Note" />
</form>